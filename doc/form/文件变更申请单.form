/*
 * 将后台传递过来的Upload file数据塞进grid中
 */
function attachAddtoGrid(file, actionType) {
    var attachInfo = new Array();
    attachInfo.push(file.oid);

    if (actionType == &quot;attach&quot;) {
        var resultData = attachGridObj.getData();
        attachInfo.push(resultData.length + 1);
        attachInfo.push(file.name);
        attachInfo.push(renderSize(file.filesize));
        attachInfo.push(&apos;1&apos;); // 临时文件

        resultData.push(attachInfo);
        attachGridObj.reload(eval(resultData));
        tAttachGrid.value = attachGridObj.toArrayString(); //將新的資料存入Grid隱藏欄位中
    } else {
        var resultData = refGridObj.getData();
        attachInfo.push(resultData.length + 1);
        attachInfo.push(file.name);
        attachInfo.push(renderSize(file.filesize));
        attachInfo.push(&apos;1&apos;);  // 临时文件

        resultData.push(attachInfo);
        refGridObj.reload(eval(resultData));
        tRefGrid.value = refGridObj.toArrayString(); //將新的資料存入Grid隱藏欄位中
    }
}

/*
 * 删除附件点击事件
 */
function attachDelete_onclick() {
    var tIndex = attachGridObj.getSelectionProperty(&quot;index&quot;);
    if (tIndex != -1) {

        if (!confirm(&quot;确定删除该文件么?&quot;)) {
            return;
        }
        var attachData = attachGridObj.getData();
        var tUrl = &quot;&quot;;
        if(attachData[tIndex][4] == &quot;1&quot;) {
            // 删除临时文件
            tUrl = &quot;/bpm-dms/tmpfile/delete/&quot; + attachData[tIndex][0];
        } else {
            // 删除正式文件
            tUrl = &quot;/bpm-dms/attachment/delete/&quot; + attachData[tIndex][0];
        }
        jQuery.ajax({
            url: tUrl,
            type: &quot;post&quot;,
            dataType: &quot;json&quot;,
            async: false,
            success: function(data) {
                if(data &gt;= 1) {
                    attachGridObj.deleteRow();
                    //在表單儲存時，把obj塞grid，是要表單重整時確定存在   
                    for(var i = 0; i &lt; attachData.length; i++) {
                        attachData[i][1] = (i + 1);
                    }

                    attachGridObj.reload(eval(attachData));
                    attachGrid.value = attachGridObj.toArrayString();
                } else {
                    alert(&quot;后台删除文件出错！&quot;);
                }
                
            },
            error: function(data) {
                alert(&quot;错误！:&quot; + data);
            }
        });

        
    } else {
        alert(&apos;请选择一笔记录！&apos;);
    }
}

