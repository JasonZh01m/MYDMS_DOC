function formSave() {
    if(activityId == &quot;requester&quot;) {
        tAttachGrid.value = attachGridObj.toArrayString();
        tRefGrid.value = refGridObj.toArrayString();
        tRecordGrid.value = recordGridObj.toArrayString();
        // return true;
        if(!allowsave) {
            alert(&apos;DMS请求错误，无法发起流程！&apos;);
            return false;
        }
        //表单保存时
        if (!checkSave()) {
            return false;
        }

         //生成文件锁
        if(!lockDocument()){
            return false;
        }

        if(document.getElementById(&quot;isRetrievalChk_0&quot;).checked) {
            document.getElementById(&apos;isRetrieval&apos;).value = 1;
        } else {
            document.getElementById(&apos;isRetrieval&apos;).value = 0;
        }
        
        if(document.getElementById(&quot;isRemindChk_0&quot;).checked) {
            document.getElementById(&apos;isRemind&apos;).value = 1;
        } else {
            document.getElementById(&apos;isRemind&apos;).value = 0;
        }

        document.getElementById(&apos;confidential&apos;).value = document.getElementById(&apos;confidentialLevel&apos;).value;
        return true;
    }
    
    if(activityId == &quot;admin&quot;) {
        // 提交ajax至bpm-dms新增dms文件
        if(!updateDocument()) {
            return false;
        }

    }
    return true;
}

