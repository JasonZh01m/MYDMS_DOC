    window.openDialog(&quot;/bpm-dms/request/uploaddialog?folderoid=&quot; + document.getElementById(&apos;folder&apos;).value + &quot;&amp;actionType=&quot; + action + &quot;&amp;gridsize=&quot; + resultData.length, 650, 450, &quot;titlebar,scrollbars,status,resizable&quot;);
