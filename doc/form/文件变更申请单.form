function loadDocument(doc) {
    document.getElementById(&apos;docNo&apos;).value = doc.serialnumber;
    document.getElementById(&apos;docName&apos;).value = doc.name;
    document.getElementById(&apos;version&apos;).value = doc.version;
    document.getElementById(&apos;author&apos;).value = doc.author;
    document.getElementById(&apos;publisherId&apos;).value = doc.creator;
    document.getElementById(&apos;creatorname&apos;).value = doc.creatorname;
    document.getElementById(&apos;effectiveDate&apos;).value = doc.effectivedate.substring(0, 10).replace(/-/g, &apos;\/&apos;);
    document.getElementById(&apos;expirationDate_txt&apos;).value = doc.expirationdate.substring(0, 10).replace(/-/g, &apos;\/&apos;);
    document.getElementById(&apos;confidential&apos;).value = doc.confidentialLevel;
    document.getElementById(&apos;confidentialLevel&apos;).value = doc.confidentialLevel;
    document.getElementById(&apos;tag&apos;).value = doc.tag == null?&quot;&quot;:doc.tag;
    document.getElementById(&apos;summary&apos;).value = doc.summary == null?&quot;&quot;:doc.summary;

    document.getElementById(&apos;isRetrieval&apos;).value = doc.isRetrieval;
    document.getElementById(&apos;isRemind&apos;).value = doc.isRemind;
    if(doc.isRemind == &quot;1&quot;) {
        document.getElementById(&quot;isRemindChk_0&quot;).checked = true;
    }

    if(doc.isRetrieval == &quot;1&quot;) {
        document.getElementById(&quot;isRetrievalChk_0&quot;).checked = true;
    }

}

function loadAttach(attachs) {
    var resultData = attachGridObj.getData();
    for(var i = 0; i &lt; attachs.length; i++) {
        var attachInfo = new Array();
        attachInfo.push(attachs[i].oid);
        attachInfo.push(i + 1);
        attachInfo.push(attachs[i].name);
        attachInfo.push(attachs[i].contenttype);
        attachInfo.push(&apos;0&apos;); // 非临时文件

        resultData.push(attachInfo);
    }
    
    attachGridObj.reload(eval(resultData));
    tAttachGrid.value = attachGridObj.toArrayString(); //將新的資料存入Grid隱藏欄位中
}

function loadRef(refs) {
