function checkPointOnClose(pReturnId){
    if(pReturnId == &quot;docNo&quot;){
        var fpid;  //文件夹PID
        var url;   //存放路径
        var tSql = &quot;select author,creatorname,createdate,folder,oid from mydms_document where serialnumber = &apos;&quot;+tDocNo.value+&quot;&apos;&quot;;
        var results = gDbConn_EFGP.query(tSql);
        if(results.length &gt; 0){
            //查找所属文件夹admin
            jQuery.ajax({
                url: &quot;/bpm-dms/folder/&quot;+results[0][3]+&quot;/admin&quot;,
                contentType: &apos;application/json;charset=utf-8&apos;,
                type: &quot;get&quot;,
                async: false,
                success: function(data) {
                    jQuery(&apos;#admin&apos;).val(data);
                },
                error: function(data) {
                    alert(&quot;错误！:&quot; + data);
                }
            });
            //路径查询
            document.getElementById(&quot;author&quot;).value = results[0][0]== &quot;null&quot;?&quot;&quot;:results[0][0];
            document.getElementById(&quot;creatorname&quot;).value = results[0][1]== &quot;null&quot;?&quot;&quot;:results[0][1];
            document.getElementById(&quot;publisherId&quot;).value = results[0][2]== &quot;null&quot;?&quot;&quot;:results[0][2];
            document.getElementById(&quot;hdndocumentOID&quot;).value = results[0][4];
            var isql = &quot;select parent,name from MYDMS_Folder where oid =&apos;&quot;+results[0][3]+&quot;&apos;&quot;;
            var result = gDbConn_EFGP.query(isql);
            fpid = result[0][0];
            url = result[0][1];
            while(fpid != &apos;null&apos;){
                var sql = &quot;select parent,name from MYDMS_Folder where id =&apos;&quot;+fpid+&quot;&apos;&quot;;
                var folderResult = gDbConn_EFGP.query(sql);
                fpid = folderResult[0][0];
                url= folderResult[0][1]+&quot;/&quot;+url;
            }
            tDmsDocUrl.value = url;
        }
    }  
}

//新增调阅记录
function addAccessInfo(){
    var flag = true;
    var retrievalRecord ={
        document:tHdndocumentOID.value,
        userid:tTxtuserId.value+&quot;_&quot;+tTxtuserName.value
    };
// console.info(&quot;retrievalRecord:{}&quot;,retrievalRecord);
    jQuery.ajax({
        url: &quot;/bpm-dms/request/doc/access/add/&quot; + userId,
        contentType: &apos;application/json;charset=utf-8&apos;,
        type: &quot;post&quot;,
        dataType: &quot;json&quot;,
        data: JSON.stringify(retrievalRecord),
        async: false,
        success: function(data) {
           if(data.code == 1) {
                document.getElementById(&quot;hdnRetrievalreOID&quot;).value = data.OID;
                flag = true;
            } else {
                flag = false;
            }
        },
        error: function(data) {
            alert(&quot;错误！:&quot;+data);
            flag = false;
        }
    });

    return flag;
}

function updateAccessInfo(){
    var flag = true;
    var retrievalreOID = document.getElementById(&quot;hdnRetrievalreOID&quot;).value;
    jQuery.ajax({
        url: &quot;/bpm-dms/request/doc/access/update/&quot;+retrievalreOID + &quot;/&quot; + userId,
        type: &quot;post&quot;,
        dataType : &quot;json&quot;,
        async: false,
        success: function(data) {
           if(data.code == 1) {
                flag = true;
            } else {
                flag = false;
                alert(data.message);
            }
        },
        error: function(data) {
            alert(&quot;错误！:&quot; + data);
            flag = false;
        }
    });
    return flag;
}
